<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="org.intellij.xquery.runner.ui.datasources.details.MarkLogicDebuggerSettingsPanel">
  <grid id="27dc6" binding="mainPanel" layout-manager="GridLayoutManager" row-count="5" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="0" left="0" bottom="0" right="0"/>
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties>
      <toolTipText value="Enable MarkLogic Debigger Connection Details Fields"/>
    </properties>
    <border type="none"/>
    <children>
      <component id="2bced" class="com.intellij.ui.components.JBCheckBox" binding="mlDebuggerEnabled">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <text value="MarkLogic Debugger Connection"/>
          <toolTipText value="Click to enable the fields for configuring a MarkLogic debugger connection"/>
        </properties>
      </component>
      <vspacer id="65bb6">
        <constraints>
          <grid row="4" column="0" row-span="1" col-span="1" vsize-policy="6" hsize-policy="1" anchor="0" fill="2" indent="0" use-parent-layout="false"/>
        </constraints>
      </vspacer>
      <component id="1860b" class="com.intellij.openapi.ui.LabeledComponent" binding="mlDebuggerPort">
        <constraints>
          <grid row="1" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="1" indent="1" use-parent-layout="false"/>
        </constraints>
        <properties>
          <componentClass value="javax.swing.JTextField"/>
          <text value="Debugger Connection Port"/>
          <toolTipText value="The port of the appserver to connect to for the debugger connection"/>
        </properties>
      </component>
      <component id="56cb" class="com.intellij.openapi.ui.LabeledComponent" binding="mlDebuggerUser">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="1" indent="1" use-parent-layout="false"/>
        </constraints>
        <properties>
          <componentClass value="javax.swing.JTextField"/>
          <text value="Debugger User"/>
          <toolTipText value="The username to connect with for the debugger connection"/>
        </properties>
      </component>
      <component id="56ce" class="com.intellij.openapi.ui.LabeledComponent" binding="mlDebuggerPassword">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="3" hsize-policy="3" anchor="0" fill="1" indent="1" use-parent-layout="false"/>
        </constraints>
        <properties>
          <componentClass value="javax.swing.JPasswordField"/>
          <text value="Debugger Password"/>
          <toolTipText value="The password for the username used for the debugger connection"/>
        </properties>
      </component>
    </children>
  </grid>
</form>
